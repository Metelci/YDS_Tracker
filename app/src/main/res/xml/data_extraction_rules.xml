<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive client-side stores -->
        <exclude domain="file" path="datastore/secure_datastore.preferences_pb"/>
        <exclude domain="file" path="datastore/calendar_settings.preferences_pb"/>
        <exclude domain="sharedpref" path="encrypted_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="file" path="datastore/secure_datastore.preferences_pb"/>
        <exclude domain="file" path="datastore/calendar_settings.preferences_pb"/>
        <exclude domain="sharedpref" path="encrypted_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>
