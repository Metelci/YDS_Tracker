    <!-- Lesson descriptions -->
    <string name="grammar_topic_description">Source: %1$s. Study in detail on this week\'s topic \'%2$s\'.</string>
    <string name="grammar_exercises_description">%1$s book exercises on yesterday\'s topic. Complete all without skipping.</string>
    <string name="grammar_reinforcement_description">Do translation or sentence construction exercises using the grammar structures you learned.</string>
    <string name="grammar_topic_continuation_description">Reinforce this week\'s grammar topic and solve additional exercises.</string>
    <string name="question_type_practice_description">Solve at least 20 questions from the %1$s question type.</string>
    <string name="weekly_grammar_review_description">Review all grammar topics and rules covered this week.</string>
    <string name="next_week_preparation_description">Briefly review next week\'s topic \'%1$s\' to prepare in advance.</string>
    <string name="weekly_analysis_planning_description">Make a general assessment of the week. Identify your strengths and weaknesses.</string>

    <!-- Exam week descriptions -->
    <string name="full_exam_description">Source: A recent YDS/YÖKDİL exam from past years. Solve all 80 questions in exactly 180 minutes.</string>
    <string name="exam_analysis_description">Take at least 1 hour break after the exam. Then thoroughly analyze your wrong answers, blanks, and correct answers you weren\'t confident about. List unknown words.</string>
    <string name="full_exam_description_2">Source: A practice exam from a reliable publisher. Solve all 80 questions in exactly 180 minutes.</string>
    <string name="exam_analysis_description_2">Perform detailed analysis like yesterday. Focus especially on recurring error types.</string>
    <string name="full_exam_description_3">Source: A past exam. Solve all 80 questions in exactly 180 minutes.</string>
    <string name="exam_analysis_description_3">Analyze your results and review new words you learned in this practice exam.</string>
    <string name="weekly_vocabulary_review_description">Review all unknown words from this week\'s practice exams using a flashcard app.</string>
    <string name="general_review_description">Review general error patterns from this week\'s practice exams (e.g., time management, specific question types).</string>
    <string name="strategy_motivation_description">Determine next week\'s strategy and relax your mind. The exam is coming soon!</string>

